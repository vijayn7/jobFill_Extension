    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "rollup": "^4.9.6",
    "tslib": "^2.8.1",
    "typescript": "^5.4.5",
