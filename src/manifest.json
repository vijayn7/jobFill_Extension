{
  "manifest_version": 3,
  "name": "JobFill Extension",
  "version": "0.1.0",
  "description": "Helps users complete job applications faster.",
  "background": {
    "service_worker": "background/index.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/index.js"],
      "css": ["content/ui/styles.css"]
    }
  ],
  "options_page": "options/options.html"
}
